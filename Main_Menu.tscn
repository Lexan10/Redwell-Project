script/source = "extends Control

onready var start_sound = $AudioStreamPlayer2

func _ready():
	start_sound.connect(\"finished\", self, \"_on_start_sound_finished\")
	BgMusic.stop()

func _on_start_pressed():
	start_sound.play()

func _on_start_sound_finished():
	BgMusic.play()
	TransitionScreen.transition()
	yield(TransitionScreen, \"on_transition_finished\")
# warning-ignore:return_value_discarded
	get_tree().change_scene(\"res://IntroNegre.tscn\")

func _on_options_pressed():
	if $Timer.is_stopped():
		$NoOptions.visible = true
		$Timer.start()

func _on_Timer_timeout():
	$NoOptions.visible = false

func _on_exit_pressed():
	get_tree().quit()

"

